import java.util.*;

public class linearsearch {

    public static int ln(int number[], int key ){
        for(int i=0; i<number.length; i++){
            if(number[i]==key){
                return i;
            }
        }
        return -1;
        
    }
    public static void main(String[] args) {
       int number[]=  {1,2,3,4,5,6,10,1,2};
       int key=4;
      int index= ln(number, key);
      if(index==-1){
        System.out.println("key is not found");
      }
      else{
        System.out.println("key is:"+index);
      }
       
    }
    
    

    }
